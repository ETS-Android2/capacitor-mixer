{
  "name": "mixer-plugin",
  "version": "0.9.0",
  "description": "Mix audio files and microphones on your device using a USB audio interface.",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "lint": "npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "docgen": "docgen --api MixerPlugin --output-readme README.md",
    "contribute-start": "npm i && npm run build"
  },
  "author": "Skylabs Technology",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^3.1.2",
    "@capacitor/core": "^3.1.2",
    "@capacitor/docgen": "0.0.17",
    "@capacitor/ios": "^3.1.2",
    "@ionic/prettier-config": "^1.0.0",
    "@ionic/swiftlint-config": "^1.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "prettier": "^2.0.5",
    "prettier-plugin-java": "^1.3.1",
    "rimraf": "^3.0.0",
    "rollup": "^2.21.0",
    "swiftlint": "^1.0.1",
    "typescript": "~3.8.3"
  },
  "peerDependencies": {
    "@capacitor/core": "^2.4.7"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "MixerPlugin.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/SkylabsTechnology/mixer-plugin"
  },
  "bugs": {
    "url": "https://github.com/SkylabsTechnology/mixer-plugin/issues"
  },
  "dependencies": {
    "@capacitor/cli": "^3.1.2"
  }
}
